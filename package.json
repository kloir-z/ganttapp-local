{
  "name": "gantt-local",
  "version": "1.0.0",
  "description": "A local offline Gantt chart application built with React, Redux, and TypeScript",
  "author": "Your Name",
  "license": "MIT",
  "homepage": "https://yourusername.github.io/gantapp-local",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/gantapp-local.git"
  },
  "keywords": [
    "gantt",
    "chart",
    "project-management",
    "react",
    "typescript",
    "offline",
    "local"
  ],
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "build:single": "echo 'Single HTML file build currently has technical issues. Use npm run build for regular build.'",
    "build:gh-pages": "cross-env NODE_ENV=production tsc && vite build --base=/gantapp-local/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "serve": "npm run build && serve -s dist",
    "generate-holidays": "node scripts/generateHolidays.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.3",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.15.7",
    "@mui/x-date-pickers": "^6.19.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@silevis/reactgrid": "^4.1.7",
    "@tippyjs/react": "^4.2.6",
    "@types/react-redux": "^7.1.33",
    "antd": "^5.18.0",
    "cdate": "^0.0.7",
    "date-fns": "^3.2.0",
    "dayjs": "^1.11.10",
    "i18next": "^23.10.1",
    "jszip": "^3.10.1",
    "moment": "^2.30.1",
    "quill": "^2.0.2",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^14.1.0",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.24.1",
    "react-select": "^5.8.0",
    "rollup": "^3.29.5",
    "styled-components": "^6.1.8",
    "tippy.js": "^6.3.7",
    "uuid": "^9.0.1",
    "vite": "^4.5.14"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.2",
    "@types/quill": "^2.0.14",
    "@types/react": "^18.2.43",
    "@types/react-color": "^3.0.11",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.5.2",
    "cross-env": "^7.0.3",
    "date-holidays": "^3.23.12",
    "depcheck": "^1.4.7",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "serve": "^14.2.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.2.2"
  }
}